ight-radius: 25px; box-shadow: 0px 1px 0px 1px #ced6e0;\">\r\n                        <b-form-input id=\"name-input\" v-model=\"email\" style=\" border-top-right-radius: 25px; border-bottom-right-radius: 25px; box-shadow: 0px 1px 0px 1px #ced6e0;\"></b-form-input>\r\n                    </b-input-group>\r\n                </b-form-group>\r\n            </form>\r\n            \r\n        </b-modal>\r\n        <!-- modal username end -->\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            nameState: null,\r\n            submittedNames: [],\r\n            username: this.compusername,\r\n            about: this.compabout,\r\n            contact: this.compcontact,\r\n            email: this.compemail,\r\n        }\r\n    },\r\n    methods: {\r\n        resetModal() {\r\n            this.name = ''\r\n            this.nameState = null\r\n        },\r\n        handleOk(bvModalEvent) {\r\n            bvModalEvent.default()\r\n            // Trigger submit handler\r\n            this.handleSubmit()\r\n        },\r\n        handleSubmit() {\r\n            // Push the name to submitted names\r\n            this.submittedNames.push(this.name)\r\n            // Hide the modal manually\r\n            this.$nextTick(() => {\r\n                this.$bvModal.hide('modal-custom-1')\r\n            })\r\n        }\r\n    },\r\n    props:{\r\n        compusername:String,\r\n        compabout:String,\r\n        compcontact:String,\r\n        compemail:String,\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.modal-body{\r\n    border-radius: 25px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}